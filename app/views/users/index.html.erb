<h1>LIST OF ALL USERS:</h1>
<table>
  <tr>
    <th>Id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%=user.id%></td>
        <td><%=user.first_name%></td>
        <td><%=user.last_name%></td>
        <td><%=user.email%></td>
      </tr>
    <%end%>
</table>
<p><%= link_to "HOME", root_path %></p>
