<div class="row">
  <div class="col-md-3">

  </div>
  <div class="col-md-6">
    <table class="table table-condensed">
      <tr>
        <th>#</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
      </tr>
        <% @users.each do |user| %>
          <tr>
            <td><%=user.id%></td>
            <td><%=user.first_name%></td>
            <td><%=user.last_name%></td>
            <td><%=user.email%></td>
          </tr>
        <%end%>
    </table>
    <p><%= link_to "HOME", root_path, class: "btn btn-default" %></p>
  </div>
  <div class="col-md-3">

  </div>
</div>
