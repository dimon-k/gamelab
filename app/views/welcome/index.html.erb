<h1>Welcome#index</h1>
<p>Find me in app/views/welcome/index.html.erb</p>
<table>
  <tr>
    <th>Id</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%=user.id%></td>
        <td><%=user.first_name%></td>
        <td><%=user.last_name%></td>
        <td><%=user.email%></td>
      </tr>
    <%end%>
</table>
