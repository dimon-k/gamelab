<div class="row">
  <div class="col-md-4">

  </div>
  <div class="col-md-4">
    <%= form_for(:session, url: login_path) do |f| %>
      <%= f.label :email %>
      <%= f.email_field :email, class: "form-control" %>
        <br />
      <%= f.label :password %>
      <%= f.password_field :password, class: "form-control" %>
        <br />
      <%= f.submit "Sign in", class: "btn btn-default" %>
    <% end %>
  </div>
  <div class="col-md-4">

  </div>
</div>
